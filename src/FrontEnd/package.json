{
  "name": "root",
  "private": true,
  "scripts": {
    "@comment build": "Optionally pass a blueprint name. IE npm run build example",
    "build": "npm-run-all \"build:server {1}\" \"build:client {1}\" --",
    "build:server": "npm-run-all \"node-build:server -- --env.BLUEPRINT={1}\" --",
    "node-build:server": "node node_modules/webpack/bin/webpack.js --config=config/webpack/prodServer.js",
    "build:client": "npm-run-all \"node-build:client -- --env.BLUEPRINT={1}\" --",
    "node-build:client": "node node_modules/webpack/bin/webpack.js --config=config/webpack/prodClient.js",
    "build:docker": "node node_modules/webpack/bin/webpack.js --config=config/webpack/prod.js",
    "create-blueprint": "node ./scripts/createBlueprint",
    "eslint": "eslint . --ext .ts,.tsx",
    "lerna:bootstrap": "lerna bootstrap --no-ci",
    "lerna:clean": "lerna clean",
    "lint:css": "ECHO use npm run stylelint",
    "postinstall": "npm run lerna:bootstrap",
    "start": "node startDevelopment.js",
    "@comment start": "Optionally pass a blueprint name and port. IE npm run start example 8080",
    "start:example": "node startDevelopment.js example 4000",
    "start:prod": "CLS && SET ISC_API_URL=http://commerce.local.com && SET ISC_FRONT_END_PORT=3000 && ECHO Starting... && node -r source-map-support/register dist/server.js",
    "start:styleguide": "npm start --prefix ./modules/mobius-styleguide",
    "stylelint": "stylelint \"./modules/**/*.tsx\"",
    "test": "cd modules && jest"
  },
  "devDependencies": {
    "@react-google-maps/api": "^1.8.2",
    "@types/codemirror": "^0.0.84",
    "@types/color": "^3.0.1",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/googlemaps": "^3.39.2",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.2.1",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/papaparse": "^4.1.34",
    "@types/qs": "^6.9.1",
    "@types/react": "16.9.13",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "7.1.3",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/set-cookie-parser": "^0.0.6",
    "@types/source-map-support": "^0.5.1",
    "@types/styled-components": "^4.4.2",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
