{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Base",
            "program": "${workspaceFolder}/startDevelopment.js",
            "cwd": "${workspaceFolder}/"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Example",
            "program": "${workspaceFolder}/startDevelopment.js",
            "cwd": "${workspaceFolder}/",
            "args": [ "example" ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Production (No Build)",
            "program": "${workspaceFolder}/dist/server.js",
            "cwd": "${workspaceFolder}/",
            "env": {
                "ISC_API_URL": "http://commerce.local.com",
                "ISC_FRONT_END_PORT": "3000",
                "ISC_ENABLE_COMPRESSION": "true"
            }
        }
    ]
}
